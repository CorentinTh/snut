{
  "name": "snut",
  "version": "0.0.1",
  "description": "Pastebin with a clean and minimalist ui.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CorentinTh/snut.git"
  },
  "keywords": [
    "share",
    "text",
    "pastebin",
    "clean",
    "self-hosted",
    "send",
    "minimalist",
    "emit",
    "transmit"
  ],
  "scripts": {
    "dev": "npm run start:dev",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run start:prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc:generateConfig": "ts-node ./scripts/generateConfig.ts",
    "release": "standard-version"
  },
  "funding": "https://github.com/sponsors/CorentinTh",
  "author": "Corentin Th <corentin.thomasset74@gmail.com> (https://github.com/CorentinTh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CorentinTh/snut/issues"
  },
  "homepage": "https://github.com/CorentinTh/snut#readme",
  "dependencies": {
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/throttler": "2.0.0",
    "@nestjs/typeorm": "8.0.2",
    "better-sqlite3": "7.4.6",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "convict": "6.2.1",
    "dotenv": "13.0.1",
    "helmet": "5.0.1",
    "pg": "8.7.1",
    "pug": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.1",
    "spectre.css": "0.5.9",
    "typeorm": "0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.6",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.4",
    "@types/compression": "1.7.2",
    "@types/convict": "6.1.1",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.19",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.5",
    "prettier": "2.5.1",
    "source-map-support": "0.5.21",
    "standard-version": "9.3.2",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
